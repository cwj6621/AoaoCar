<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 手动选择的程度圆-->
    <declare-styleable name="CustomProgressBar">
        <attr name="circleWidth" format="dimension" />
        <attr name="ringColor" format="color" />
        <attr name="centercircleColor" format="color" />
        <attr name="textColor" format="color" />
    </declare-styleable>
    <!-- 优惠券-->
    <declare-styleable name="CouponView">
        <attr name="cv_semicircle_gap" format="dimension|reference"/>
        <attr name="cv_semicircle_radius" format="dimension|reference"/>
        <attr name="cv_semicircle_color" format="color|reference"/>
        <attr name="cv_dash_line_length" format="dimension|reference"/>
        <attr name="cv_dash_line_gap" format="dimension|reference"/>
        <attr name="cv_dash_line_height" format="dimension|reference"/>
        <attr name="cv_dash_line_color" format="color|reference"/>
        <attr name="cv_is_semicircle_top" format="boolean|reference"/>
        <attr name="cv_is_semicircle_bottom" format="boolean|reference"/>
        <attr name="cv_is_semicircle_left" format="boolean|reference"/>
        <attr name="cv_is_semicircle_right" format="boolean|reference"/>
        <attr name="cv_is_dash_line_top" format="boolean|reference"/>
        <attr name="cv_is_dash_line_bottom" format="boolean|reference"/>
        <attr name="cv_is_dash_line_left" format="boolean|reference"/>
        <attr name="cv_is_dash_line_right" format="boolean|reference"/>
        <attr name="cv_dash_line_margin_top" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_bottom" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_left" format="dimension|reference"/>
        <attr name="cv_dash_line_margin_right" format="dimension|reference"/>
    </declare-styleable>

    <!-- RoundImageView -->
    <declare-styleable name="round_image_view">
        <attr name="border_out_width" format="dimension|reference" />
        <attr name="border_in_width" format="dimension|reference" />
        <attr name="border_outcolor" format="color|reference" />
        <attr name="border_incolor" format="color|reference" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
    </declare-styleable>

    <declare-styleable name="SuperCircleView">
        <!-- 圆的半价 -->
        <attr name="min_circle_radio" format="integer"></attr>
        <!-- 圆环的宽度 -->
        <attr name="ring_width" format="float"></attr>
        <!-- 分成几段 -->
        <attr name="select" format="integer"></attr>
        <!-- 每段间隔的角度 -->
        <attr name="selec_angle" format="integer"></attr>
        <!-- 圆的颜色 -->
        <attr name="circle_color" format="color"></attr>

        <!-- 圆的颜色 -->
        <attr name="max_circle_color" format="color"></attr>

        <!-- 圆环的默认颜色 -->
        <attr name="ring_normal_color" format="color"></attr>

        <!-- 圆环要显示的彩色的区域-->
        <attr name="ring_color_select" format="integer"></attr>

        <!-- 是否显示断 -->
        <attr name="is_show_select" format="boolean"></attr>
    </declare-styleable>

    <declare-styleable name="SlideBottomPanel">
        <attr name="sbp_background_layout" format="reference"/>
        <attr name="sbp_title_height_no_display" format="dimension"/>
        <attr name="sbp_panel_height" format="dimension"/>
        <attr name="sbp_boundary" format="boolean"/>
        <attr name="sbp_move_distance_trigger" format="dimension"/>
        <attr name="sbp_animation_duration" format="integer"/>
        <attr name="sbp_fade" format="boolean"/>
        <attr name="sbp_hide_panel_title" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="easy_flip_view">
        <attr name="flipOnTouch" format="boolean"/>
        <attr name="flipDuration" format="integer"/>
        <attr name="flipType" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="flipEnabled" format="boolean"/>
        <attr name="animFlipInId" format="reference"/>
        <attr name="animFlipOutId" format="reference"/>
    </declare-styleable>


    <declare-styleable name="LinkBuilder">
        <attr name="defaultLinkColor" format="color" />
        <attr name="defaultTextColorOfHighlightedLink" format="color" />
    </declare-styleable>

    <attr name="linkBuilderStyle" format="reference" />


    <declare-styleable name="SimpleRatingBar">
        <attr name="srb_borderColor" format="color" />
        <attr name="srb_fillColor" format="color" />
        <attr name="srb_backgroundColor" format="color" />
        <attr name="srb_starBackgroundColor" format="color" />
        <attr name="srb_pressedBorderColor" format="color" />
        <attr name="srb_pressedFillColor" format="color" />
        <attr name="srb_pressedBackgroundColor" format="color" />
        <attr name="srb_pressedStarBackgroundColor" format="color" />
        <attr name="srb_numberOfStars" format="integer" />
        <attr name="srb_starSize" format="dimension" />
        <attr name="srb_maxStarSize" format="dimension" />
        <attr name="srb_starsSeparation" format="dimension" />
        <attr name="srb_starBorderWidth" format="float" />
        <attr name="srb_starCornerRadius" format="float" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_stepSize" format="float" />
        <attr name="srb_drawBorderEnabled" format="boolean" />
        <attr name="srb_gravity" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleIndicator">
        <attr name="ci_radius" format="dimension"/>
        <attr name="ci_margin" format="dimension"/>
        <attr name="ci_background" format="color|integer"/>
        <attr name="ci_selected_background" format="color|integer"/>
        <attr name="ci_gravity">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
        <attr name="ci_mode">
            <enum name="inside" value="0"/>
            <enum name="outside" value="1"/>
            <enum name="solo" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MagicProgressBar">
        <attr name="mpb_percent" format="float" />
        <attr name="mpb_fill_color" format="color" />
        <attr name="mpb_background_color" format="color" />
        <attr name="mpb_flat" format="boolean" />

    </declare-styleable>

    <declare-styleable name="TextStepView">
        <attr name="prostep" format="integer" />
        <attr name="default_color" format="color" />
        <attr name="selected_color" format="color" />
        <attr name="text_padding" format="dimension" />
    </declare-styleable>
</resources>
